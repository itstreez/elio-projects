AC,CAAC,CAAC,CAAC","file":"/Users/eliomouhasseb3/chatprojet/src/components/design.vue","sourceRoot":"","sourcesContent":["<template> \n  <body>\n    <!--  container -->\n\n    <!--  title -->\n    <div id=\"big-box\">\n      <div id=\"search-container\">\n        <input type=\"text\" v-model=\"search\" size=\"15\" placeholder=\"Search ..\" />\n      </div>\n      <div id=\"conversation-list\">\n        <div\n          v-on:click=\"display(user.id)\"\n          v-bind:key=\"user.id\"\n          v-for=\"user in filteredSearch\"\n          v-bind:class=\"{ conversation: current === user.id }\"\n          class=\"conversation\"\n          id=\"conversation\"\n        >\n          <img v-bind:src=\"user.photo\"/>\n          <div class=\"title-text\">\n            {{ user.name }}\n          </div>\n          <div class=\"created-date\">\n            {{ user.created }}\n          </div>\n          <div class=\"conversation-message\">\n            {{ user.message }}\n          </div>\n        </div>\n      </div>\n      <div id=\"page-of\">\n        <h2>\n          <button v-on:click=\"increment\" :disabled=\"true\" value=\"<\"> &lt;</button>\n        </h2>\n        <h1>{{ counter }} of 1</h1>\n        <h3>\n          <button v-on:click=\"increment\" :disabled=\"true\" value=\">\">></button>\n        </h3>\n      </div>\n      <div id=\"chat-title\">\n        <h1>{{ valtitle }}</h1>\n        <h3 id=\"lasts\">  {{lasts}} </h3>\n\n      </div>\n      <div id=\"chat-message-list\">\n        <h2 id=\"ms1\" class=\"other1\">{{ ms1 }}</h2>\n        <h2 id=\"ms2\" class=\"you1\">{{ ms2 }}</h2>\n        <h2 id=\"ms3\" class=\"other2\">{{ ms3 }}</h2>\n        <h2 id=\"ms4\" class=\"you2\">{{ ms4 }}</h2>\n        <div id=\"inserted\" v-for=\"(message, index) in insertedMessages\"  v-bind:key=\"index\"  > \n <h2 id=\"newmessage\"> {{message}}</h2>\n        </div>\n\n        <!-- i couldn't do the open conv , because the div is\n     outside the main div , if i copy paste it inside the main div , the interface will crash -->\n      </div>\n      <div id=\"chat-form\">\n        <input @keyup.enter=\"addMessage()\"\n          v-bind:title=\"message\"\n           v-model=\"inputMessage\"\n          type=\"text\"\n          placeholder=\"type a message and press enter\"\n          size=\"96\"\n        />\n       \n      </div>\n    </div>\n  </body>\n</template>\n\n<script>\nimport axios from 'axios';\nconst urlusers = \"http://localhost:3000/users\";\nconst urlmess = \"http://localhost:3000/messages\"\nexport default {\n  data() {\n            return {\n               inputMessage: \"\",\n                ms1: 'Please',\n                ms2: 'Run',\n                ms3:'Json',\n                ms4:'Server',\n                valtitle:'no chat selected', \n                lasts: '' , \n                current: null,\n                value1: 'no chat selected',\n                message: ' You at ' + new Date().toLocaleString(),\n                counter: 1,\n                insertedMessages: [\n\n                ] , \n                messages: [],\n                users: []\n                ,\n                search: ''\n            }\n\n\n        },\n        async created(){\ntry{\n  const resp1 = await axios.get(urlusers);\n   const resp2 = await axios.get(urlmess);\n\n  this.users = resp1.data;\n  this.messages = resp2.data;\n}\ncatch(e){\n  console.error(e);\n}\n} , \n        methods: {\n          addMessage(){\n           \n           \nif (this.inputMessage == \"\") {\n        alert(\"Please insert some text.\")\n        \n      }\n      else {\n       \nthis.insertedMessages.push(this.inputMessage);\n       this.inputMessage='';\n        }\n\n       \n      \n          },\n            increment() {\n                if (this.counter == 1) {\n                    this.counter++\n                    alert(\"hi\");\n                }\n                else if (this.counter == 2) {\n                    this.counter--\n                }\n\n            \n 